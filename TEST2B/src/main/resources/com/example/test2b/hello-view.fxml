<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="215.0" prefWidth="454.0" spacing="20.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/21" fx:controller="com.example.test2b.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>

    <Label fx:id="welcomeText" />
   <TextField fx:id="uid"/>
   <TextField fx:id="uname"/>
   <TextField fx:id="uquantity"/>
   <TextField fx:id="uprice"/>
   <TableView fx:id="tableview" prefHeight="200.0" prefWidth="200.0">
     <columns>
       <TableColumn fx:id="item_id" prefWidth="75.0" text="item_id" />
       <TableColumn fx:id="item_name" prefWidth="75.0" text="item_name" />
         <TableColumn fx:id="item_quantity" prefWidth="75.0" text="item_quantity" />
         <TableColumn fx:id="item_price" prefWidth="75.0" text="item_price" />
     </columns>
   </TableView>
    <Button onAction="#fetchdata"  text="fetch" />
   <Button mnemonicParsing="false" onAction="#insertdata" text="Insert" />
   <Button mnemonicParsing="false" onAction="#updatedata" text="Update" />
   <Button mnemonicParsing="false" onAction="#deletedata" text="Delete" />
   <Button mnemonicParsing="false" onAction="#loaddata" text="Load" />
</VBox>
